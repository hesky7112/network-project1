{
    "id": "network-troubleshooter-bot",
    "name": "Network Troubleshooting Bot",
    "version": "1.0.0",
    "description": "AI chatbot that diagnoses network issues and suggests fixes using RAG.",
    "category": "network",
    "tags": [
        "network",
        "chatbot",
        "ai",
        "troubleshooting"
    ],
    "execution_mode": "server",
    "primitives": [
        {
            "module": "ChatbotEngine",
            "method": "chat",
            "config": {
                "kb_id": "network_knowledge"
            }
        },
        {
            "module": "DataIngestion",
            "method": "fetch_internal_api",
            "config": {
                "endpoint": "/api/v1/telemetry/live"
            }
        }
    ],
    "ui": {
        "form_fields": [
            {
                "name": "query",
                "type": "text",
                "label": "What's the issue?",
                "required": true
            }
        ],
        "output_type": "chat"
    },
    "pricing": {
        "license_type": "lease",
        "price": 54,
        "currency": "USD"
    },
    "requirements": {
        "requires_hal": false,
        "requires_gpu": false,
        "wasm_compatible": false
    },
    "author": {
        "name": "AlienNet Ops"
    }
}